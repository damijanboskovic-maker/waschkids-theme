// Funktion zur Erkennung von Mobile-GerÃ¤ten
function isMobileDevice() {
  return /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ||
         (window.innerWidth <= 768 && 'ontouchstart' in window);
}

// Funktion um Ready2Wash Featured Products Sektion zu finden
function findReady2WashSection() {
  // Suche nach allen .featured-products Sektionen
  const featuredSections = document.querySelectorAll('.featured-products');
  
  for (const section of featuredSections) {
    // PrÃ¼fe die Ãœberschrift (h2.featured-products__heading)
    const heading = section.querySelector('h2.featured-products__heading');
    if (heading && heading.textContent.includes('Ready2Wash')) {
      console.log('âœ… Ready2Wash Featured Products Sektion gefunden');
      return section;
    }
  }
  
  console.log('âš ï¸ Ready2Wash Sektion nicht gefunden');
  return null;
}

