  console.log('ðŸŒŠ Initialisiere Wassereffekte fÃ¼r Ready2Wash Featured Products...');
  
  // Finde die Ready2Wash Sektion
  const ready2WashSection = findReady2WashSection();
  
  if (!ready2WashSection) {
    console.log('âŒ Ready2Wash Featured Products Sektion nicht gefunden - keine Effekte angewendet');
    return;
  }
  
  // Suche nur Produktkarten in dieser Sektion (exakte Selektoren aus Liquid)
  const cards = ready2WashSection.querySelectorAll('.collection-product-card');
  console.log(`âœ… ${cards.length} Produktkarten in Ready2Wash Sektion gefunden`);
  
  if (cards.length === 0) {
    console.log('âš ï¸ Keine Produktkarten in Ready2Wash Sektion gefunden');
    return;
  }
  
  const isMobile = isMobileDevice();
  console.log(`ðŸ“± Mobile GerÃ¤t erkannt: ${isMobile}`);
  
  cards.forEach((card, index) => {
    card.style.position = 'relative';
    card.style.overflow = 'visible';
    
    // Container fÃ¼r alle Effekte
    const container = document.createElement('div');
    container.className = 'fancy-water-container';
    
    if (isMobile) {
      // Mobile: Effekte immer sichtbar
      container.style.cssText = `
        position: absolute !important;
