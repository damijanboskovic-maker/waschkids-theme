  content: '' !important;
  position: absolute !important;
  top: 0 !important;
  left: -100% !important;
  width: 100% !important;
  height: 100% !important;
  background: linear-gradient(90deg, transparent, rgba(255,255,255,0.4), transparent) !important;
  animation: shimmer 4s infinite !important;
  z-index: 1 !important;
}

@keyframes shimmer {
  0% { left: -100%; }
  100% { left: 100%; }
}

/* Text-Effekt fÃ¼r die Announcement-Bar */
.announcement-bar p,
.announcement-bar span,
.announcement-bar-1 p,
.announcement-bar-1 span {
  text-shadow: 0 0 8px rgba(255,255,255,0.6) !important;
  animation: textGlow 3s ease-in-out infinite alternate !important;
  position: relative !important;
  z-index: 2 !important;
}

@keyframes textGlow {
  0% { 
    text-shadow: 0 0 5px rgba(255,255,255,0.4);
    transform: scale(1);
  }
  100% { 
    text-shadow: 0 0 15px rgba(255,255,255,0.8);
    transform: scale(1.02);
  }
}

/* Hover-Effekt fÃ¼r die gesamte Announcement-Bar */
.announcement-bar:hover,
.announcement-bar-1:hover {
  animation-play-state: paused !important;
  background: linear-gradient(45deg, #1e40af, #2563eb, #3b82f6, #60a5fa, #3b82f6, #2563eb, #1e40af) !important;
}
</style>

