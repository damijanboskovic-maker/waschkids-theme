// Fancy Effekte generieren (gleiche Funktion wie vorher)
function generateFancyEffects(container, card) {
  const rect = card.getBoundingClientRect();
  const centerX = rect.width / 2;
  const centerY = rect.height / 2;
  const isMobile = isMobileDevice();
  
  // Mobile: Weniger intensive Effekte fÃ¼r bessere Performance
  const bubbleCount = isMobile ? 2 : 4;
  const splashChance = isMobile ? 0.4 : 0.6;
  const shimmerChance = isMobile ? 0.2 : 0.3;
  const sparkleChance = isMobile ? 0.1 : 0.2;
  
  // 1. FANCY WASSERBLASEN
  for (let i = 0; i < bubbleCount; i++) {
    const bubble = document.createElement('div');
    bubble.className = 'water-bubble';
    const size = Math.random() * (isMobile ? 12 : 16) + (isMobile ? 6 : 8);
    const x = Math.random() * rect.width;
    const delay = Math.random() * 1;
    
    bubble.style.cssText = `
      width: ${size}px;
      height: ${size}px;
      left: ${x}px;
      top: ${rect.height}px;
      animation: bubbleFloat ${2.5 + Math.random()}s ease-out forwards;
      animation-delay: ${delay}s;
    `;
    
    container.appendChild(bubble);
    
    setTimeout(() => {
      if (bubble.parentNode) bubble.parentNode.removeChild(bubble);
    }, 4000);
  }
  
  // 2. FANCY WASSERSPRITZER
  if (Math.random() > (1 - splashChance)) {
    const splashCount = isMobile ? 4 : 6;
    for (let i = 0; i < splashCount; i++) {
      const splash = document.createElement('div');
      splash.className = 'water-splash';
      const size = Math.random() * (isMobile ? 8 : 10) + (isMobile ? 4 : 6);
      const angle = (i / splashCount) * 360;
      const distance = Math.random() * (isMobile ? 40 : 50) + (isMobile ? 20 : 25);
      const x = centerX + Math.cos(angle * Math.PI / 180) * distance;
      const y = centerY + Math.sin(angle * Math.PI / 180) * distance;
      
      splash.style.cssText = `
        width: ${size}px;
        height: ${size}px;
        left: ${x - size/2}px;
        top: ${y - size/2}px;
        animation: gentleSplash ${1.2 + Math.random() * 0.6}s ease-out forwards;
      `;
      
      container.appendChild(splash);
      
      setTimeout(() => {
        if (splash.parentNode) splash.parentNode.removeChild(splash);
      }, 1800);
    }
  }
  
  // 3. SHIMMER-EFFEKT
  if (Math.random() > (1 - shimmerChance)) {
    const shimmer = document.createElement('div');
    shimmer.className = 'shimmer-effect';
    shimmer.style.cssText = `
      animation: shimmer ${1.8}s ease-out forwards;
    `;
    
    container.appendChild(shimmer);
    
    setTimeout(() => {
      if (shimmer.parentNode) shimmer.parentNode.removeChild(shimmer);
    }, 1800);
  }
  
  // 4. SPARKLE-EFFEKT
  if (Math.random() > (1 - sparkleChance)) {
    const sparkleCount = isMobile ? 2 : 3;
    for (let i = 0; i < sparkleCount; i++) {
      const sparkle = document.createElement('div');
      sparkle.className = 'sparkle-effect';
      const x = Math.random() * rect.width;
      const y = Math.random() * rect.height;
      
      sparkle.style.cssText = `
        left: ${x}px;
        top: ${y}px;
        animation: sparkle ${1 + Math.random() * 0.5}s ease-out forwards;
        animation-delay: ${Math.random() * 0.5}s;
      `;
      
      container.appendChild(sparkle);
      
      setTimeout(() => {
        if (sparkle.parentNode) sparkle.parentNode.removeChild(sparkle);
      }, 1500);
    }
  }
}

// Starte nach DOM-Load
if (document.readyState === 'loading') {
  document.addEventListener('DOMContentLoaded', () => {
    setTimeout(initFancyWaterEffects, 1000);
  });
} else {
